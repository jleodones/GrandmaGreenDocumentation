.item-button {
    width: 250px;
    height: 250px;
    border-top-left-radius: 50px;
    border-bottom-left-radius: 50px;
    border-top-right-radius: 50px;
    border-bottom-right-radius: 50px;
    padding-left: 40px;
    padding-right: 40px;
    padding-top: 40px;
    padding-bottom: 40px;
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    position: absolute;
    top: 10px;
}

.popup-box {
    width: 1150px;
    height: 800px;
    background-color: rgba(255, 255, 255, 0);
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 20px;
    padding-bottom: 20px;
    background-image: url('project://database/Assets/_GrandmaGreen/Art/UI/Shopping/UI_Plant%20Shop_PopUp_BG.png?fileID=2800000&guid=1df3e73dc413ec3468c40d9264a84c0c&type=3#UI_Plant Shop_PopUp_BG');
    -unity-background-scale-mode: scale-to-fit;
    justify-content: center;
}

.price-box {
    width: 200px;
    height: 100px;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    position: absolute;
    bottom: 5%;
}

.price-text {
    font-size: 300%;
    color: rgb(114, 67, 37);
    -unity-text-align: middle-center;
    width: 60%;
    height: 100%;
    -unity-font-style: bold;
    -unity-font: url('project://database/Assets/_GrandmaGreen/Art/UI/Fonts/Brevia/Brevia-Regular.otf?fileID=12800000&guid=5a1bfd81ed39ecd418c1c034114fcc3c&type=3#Brevia-Regular');
