    background-color: rgba(91, 163, 167, 0);
    background-image: url('project://database/Assets/_GrandmaGreen/Art/UI/Shopping/UI_Shop_PopUp_Buy%20Button.png?fileID=2800000&guid=515905bc0077a2944ba969dd1febe12a&type=3#UI_Shop_PopUp_Buy Button');
