}

.price-icon {
    background-image: url('project://database/Assets/_GrandmaGreen/Art/UI/Icons/HUD/UI_HUD_Gold%201.png?fileID=2800000&guid=a905f87446937e343bfa7472f3e2bb77&type=3#UI_HUD_Gold 1');
    margin-left: 5px;
    margin-right: 5px;
    margin-top: 5px;
    margin-bottom: 5px;
    width: 25%;
    height: 100%;
    -unity-background-scale-mode: scale-to-fit;
}

.currency-box {
    width: 200px;
    height: 80px;
    background-color: rgb(232, 241, 228);
    border-top-left-radius: 50px;
    border-bottom-left-radius: 50px;
    border-top-right-radius: 50px;
    border-bottom-right-radius: 50px;
    padding-left: 15px;
    padding-right: 15px;
    padding-top: 15px;
    padding-bottom: 15px;
    align-items: stretch;
    justify-content: space-between;
}

.currency-icon {
    width: 50px;
    height: 50px;
}

.currency-text {
    font-size: 300%;
    color: rgb(91, 163, 167);
    -unity-text-align: middle-right;
    -unity-font-style: bold;
    -unity-font: url('project://database/Assets/_GrandmaGreen/Art/UI/Fonts/Brevia/Brevia-Regular.otf?fileID=12800000&guid=5a1bfd81ed39ecd418c1c034114fcc3c&type=3#Brevia-Regular');
